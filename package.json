{
  "name": "reactour",
  "version": "1.2.1",
  "description": "Tourist Guide into your React Components",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run lib",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config webpack.config.babel.js",
